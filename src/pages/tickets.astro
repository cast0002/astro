---
import GalleryImg from "../compontents/GalleryImg.astro";
import Navigation from "../compontents/Navigation.astro";
import Footer from "../compontents/Footer.astro";
---

<Navigation />
<style>
   body {
      margin: 0;
      padding: 0;
      background-color: black;
      background-image: url("/backgroundkarrusel.png");
      background-repeat: no-repeat;
      background-size: cover;
      background-position: center;
      height: 100vh;
   }
   .photo-container {
      max-width: 50%;
      max-height: 80%;
      overflow: hidden;
      text-align: center;
      margin: auto;
      padding-top: 15%;
      display: flex;
      justify-content: center;
      align-items: center;
      padding-bottom: 30%;
   }
   .photo {
      max-width: 100%;
      max-height: 100%;
      object-fit: contain;
   }
</style>

<body>
   <div class="photo-container">
      <img class="photo" src="/public/ticketsfoto.png" alt="Tickets" />
   </div>
</body>

<Footer />
